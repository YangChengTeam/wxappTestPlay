<!--components/nav/nav.wxml-->
<basenav background="{{background}}" color="{{color}}">
  <view hidden='{{hidden}}' class="back" wx:if="{{is_back}}">
    <view class="back-wrapper" bindtap='navigateBack'>
      <image src="../../assets/images/{{back_icon_path}}"></image>
    </view>
  </view>
  <view hidden='{{hidden}}'>{{title}}</view>
</basenav>