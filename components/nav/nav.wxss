/* components/nav/nav.wxss */
.back {
   width: 100%;
   height: 100%;
   display: flex;
   align-items: center;
   position: absolute;
   left: 0;
   top: 0;
   z-index: 2;
   box-shadow: 0px 1px 0px rgba(0, 0, 0, .1)
}

.back-wrapper {
   height: 100%;
   display: flex;
   align-items: center;
}

.back image {
   width: 34rpx;
   height: 34rpx;
   margin-left: 20rpx;
}

